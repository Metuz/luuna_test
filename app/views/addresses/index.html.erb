<h1>Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>Zip code</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.street %></td>
        <td><%= address.zip_code %></td>
        <td><%= address.city %></td>
        <td><%= address.state %></td>
        <td><%= address.country %></td>
        <td><%= address.user_name %></td>
        <td><%= show_link address %></td>
        <td><%= edit_link address %></td>
        <td><%= destroy_link address %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= create_link Address %>
